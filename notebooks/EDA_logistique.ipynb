    "\n",
    "# --- 1. CHARGEMENT & PRÉPARATION (Identique à l'original) ---\n",
    "try:\n",
    "    df_lits = pd.read_csv('../data/raw/lits_poles.csv')\n",
    "    df_perso = pd.read_csv('../data/raw/personnel_poles.csv')\n",
    "    df_equip = pd.read_csv('../data/raw/equipements_poles.csv')\n",
    "    df_stocks = pd.read_csv('../data/raw/stocks_medicaments.csv')\n",
    "except FileNotFoundError:\n",
    "    # Création de données dummy si les fichiers n'existent pas (pour l'exemple)\n",
    "    import numpy as np\n",
    "    print(\"⚠️ Fichiers non trouvés, génération de données simulées...\")\n",
    "    dates = pd.date_range('2023-01-01', periods=100)\n",
    "    df_lits = pd.DataFrame({'date': dates, 'service': np.random.choice(['Cardio', 'Neuro', 'Urgences', 'Onco', 'Geriatrie'], 100), 'lits_occupes': np.random.randint(80, 100, 100), 'lits_totaux': 100})\n",
    "    df_perso = pd.DataFrame({'date': dates, 'categorie': np.random.choice(['Infirmier', 'Medecin', 'AS', 'Admin'], 100), 'effectif_total': np.random.randint(10, 50, 100), 'taux_absence': np.random.uniform(0.01, 0.1, 100)})\n",
    "    df_equip = pd.DataFrame({'date': dates, 'service': 'General', 'effectif_present': 90, 'effectif_total': 100})\n",
    "    df_stocks = pd.DataFrame({'date': dates, 'medicament': np.random.choice(['Doliprane', 'Adrenaline', 'Morphine', 'Saline'], 100), 'alerte_rupture': np.random.choice([True, False], 100, p=[0.2, 0.8])})\n",
    "\n",
    "# Dates\n",
    "for df in [df_lits, df_perso, df_equip, df_stocks]:\n",
    "    df['date'] = pd.to_datetime(df['date'])\n",
    "\n",
    "# Taux d'occupation\n",
    "df_lits['taux_occupation'] = df_lits['lits_occupes'] / df_lits['lits_totaux']\n",
    "\n",
    "# --- 2. ANALYSE TEXTUELLE (Identique) ---\n",
    "print(\"PITIÉ-SALPÊTRIÈRE - EDA logistique (Version Plotly)\")\n",
    "# (Calculs identiques à votre script)\n",
    "perf_poles = df_lits.groupby('service')['taux_occupation'].agg(['mean','max']).round(2)\n",
    "perf_poles.columns = ['Moyenne', 'PIC']\n",
    "\n",
    "print(f\"Occupation moyenne : {df_lits['taux_occupation'].mean():.1%}\")\n",
    "print(f\"Alertes stocks     : {df_stocks['alerte_rupture'].sum():,}\")\n",
    "\n",
    "# --- 3. VISUALISATION PLOTLY ---\n",
    "\n",
    "# Création de la structure du Dashboard (2 lignes, 2 colonnes)\n",
    "# Note : Le Pie chart nécessite un type 'domain'\n",
    "fig = make_subplots(\n",
    "    rows=2, cols=2,\n",
    "    subplot_titles=('Pôles Critiques (Moyenne vs PIC)', 'Répartition Effectifs', \n",
    "                    'Top Ruptures Stocks', 'Saisonnalité Occupation'),\n",
    "    specs=[[{\"type\": \"xy\"}, {\"type\": \"domain\"}],\n",
    "           [{\"type\": \"xy\"}, {\"type\": \"xy\"}]]\n",
    ")\n",
    "\n",
    "# A. Graphique 1 : Pôles Critiques (Barres superposées)\n",
    "top_poles = perf_poles.sort_values('PIC', ascending=False).head(5)\n",
    "\n",
    "# Barre PIC (Arrière-plan)\n",
    "fig.add_trace(go.Bar(\n",
    "    x=top_poles.index, \n",
    "    y=top_poles['PIC'],\n",
    "    name='PIC Max',\n",
    "    marker_color='red',\n",
    "    opacity=0.6\n",
    "), row=1, col=1)\n",
    "\n",
    "# Barre Moyenne (Devant)\n",
    "fig.add_trace(go.Bar(\n",
    "    x=top_poles.index, \n",
    "    y=top_poles['Moyenne'],\n",
    "    name='Moyenne',\n",
    "    marker_color='orange',\n",
    "    opacity=1.0\n",
    "), row=1, col=1)\n",
    "\n",
    "# B. Graphique 2 : Personnel (Pie Chart)\n",
    "perso_cat = df_perso[df_perso['categorie'] != 'total'].groupby('categorie')['effectif_total'].sum()\n",
    "\n",
    "fig.add_trace(go.Pie(\n",
    "    labels=perso_cat.index,\n",
    "    values=perso_cat.values,\n",
    "    name=\"Effectifs\",\n",
    "    hole=0.3, # Optionnel : transforme en Donut chart pour plus de modernité\n",
    "    marker=dict(colors=['#636EFA', '#EF553B', '#00CC96', '#AB63FA'])\n",
    "), row=1, col=2)\n",
    "\n",
    "# C. Graphique 3 : Ruptures (Barres Horizontales)\n",
    "ruptures = df_stocks[df_stocks['alerte_rupture']==True]['medicament'].value_counts().head(4)\n",
    "# On inverse l'ordre pour que le plus grand soit en haut graphiquement\n",
    "ruptures = ruptures.sort_values(ascending=True) \n",
    "\n",
    "fig.add_trace(go.Bar(\n",
    "    y=ruptures.index,\n",
    "    x=ruptures.values,\n",
    "    orientation='h',\n",
    "    name='Ruptures',\n",
    "    marker_color='crimson',\n",
    "    text=ruptures.values,\n",
    "    textposition='auto'\n",
    "), row=2, col=1)\n",
    "\n",
    "# D. Graphique 4 : Saisonnalité (Line Chart)\n",
    "df_lits['mois'] = df_lits['date'].dt.month\n",
    "saison = df_lits.groupby('mois')['taux_occupation'].mean()\n",
    "\n",
    "fig.add_trace(go.Scatter(\n",
    "    x=saison.index,\n",
    "    y=saison.values,\n",
    "    mode='lines+markers',\n",
    "    name='Occupation',\n",
    "    line=dict(color='royalblue', width=3),\n",
    "    marker=dict(size=8)\n",
    "), row=2, col=2)\n",
    "\n",
    "# --- 4. MISE EN FORME DU LAYOUT ---\n",
    "fig.update_layout(\n",
    "    title_text=\"Dashboard Logistique Hospitalière (Interactif)\",\n",
    "    height=900,\n",
    "    width=1300,\n",
    "    showlegend=True,\n",
    "    template=\"plotly_white\", # Fond blanc propre\n",
    "    barmode='overlay' # Important pour le graph 1 (superposition)\n",
    ")\n",
    "\n",
    "# Ajustement spécifique des axes\n",
    "fig.update_xaxes(title_text=\"Mois\", row=2, col=2)\n",
    "fig.update_yaxes(range=[0, 1.1], row=2, col=2) # Fixe l'échelle de 0 à 110%\n",
    "\n",
    "# Sauvegarde et Affichage\n",
    "print(\"\\n... Génération du fichier HTML interactif ...\")\n",
    "fig.write_html(\"dashboard_logistique.html\")\n",
    "fig.show()\n",
    "\n",
    "# --- 5. INSIGHTS (Identique) ---\n",
    "print(\"\\nINSIGHTS SOUTENANCE:\")\n",
    "print(f\"• {perf_poles.index[0]}: {perf_poles['PIC'].max():.0%} PIC MAX\")\n",
    "print(f\"• {df_stocks['alerte_rupture'].sum():,} alertes stocks\")\n",
    "if not perso_cat.empty:\n",
    "    print(f\"• {perso_cat.idxmax()} domine: {perso_cat.max():,} ETP\")"
   ]
  },
